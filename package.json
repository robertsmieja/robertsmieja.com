    "eslint": "^7.1.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "npm-run-all": "^4.1.5",
    "post-npm-install": "^2.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.3",
    "utility-types": "^3.10.0"
  },
  "keywords": [
    "gatsby",
    "robert",
    "robertsmieja",
    "smieja"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "check": "run-p type-check lint format",
    "clean": "rimraf public/ .cache/",
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,md,json}\" \"*.{js,json,md}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"*.js\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertsmieja/robertsmieja.com"
  },
  "bugs": {
    "url": "https://github.com/robertsmieja/robertsmieja.com/issues"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ],
      "post-merge": [
        "post-npm-install"
      ],
      "post-rebase": [
        "post-npm-install"
      ]
    }
  }
}
