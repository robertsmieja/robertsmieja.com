    "gatsby-plugin-sitemap": "^2.2.14",
    "gatsby-plugin-typescript": "^2.1.11",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^2.1.26",
    "gatsby-transformer-sharp": "^2.2.17",
    "husky": "^3.0.5",
    "lint-staged": "^9.4.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0"
  },
  "keywords": [
    "gatsby",
    "robert",
    "robertsmieja",
    "smieja"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,md,json}' '*.{js,json,md}'",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robertsmieja/robertsmieja.com"
  },
  "bugs": {
    "url": "https://github.com/robertsmieja/robertsmieja.com/issues"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  }
}
