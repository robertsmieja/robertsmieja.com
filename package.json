  },
  "bugs": {
    "url": "https://github.com/robertsmieja/robertsmieja.com/issues"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ],
      "post-merge": [
        "post-npm-install"
      ],
      "post-rebase": [
        "post-npm-install"
      ]
    }
